<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.2.xsd">
	<context:annotation-config/>
	<bean id="report" class="com.dgit.ex.Report">
		<property name="grade1" value="95"/>
		<property name="grade2" value="85"/>
		<property name="grade3" value="75"/>
		<property name="grade4" value="65"/>
	</bean>
	<bean id="family" class="com.dgit.ex.Family">
		<constructor-arg name="father" value="이쁘니 아빠"/>
		<constructor-arg name="mother" value="이쁘니 엄마"/>
		<property name="sister" value="여동생"/>
	</bean>
	<bean id="student" class="com.dgit.ex.StudentInfo">
		<property name="name" value="이쁘니"/>
		<property name="stdClass" value="2"/>
		<property name="math" value="100"/>
		<property name="eng" value="88"/>
		<property name="kor" value="91"/>
	</bean>
</beans>
